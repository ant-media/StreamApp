    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "rimraf": "^5.0.5",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.10.0",
    "vitest": "^2.1.9"
  },
  "dependencies": {
    "prettier": "^3.6.2"
  }
